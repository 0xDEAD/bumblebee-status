            "good": {
                "fg": "#002b36",
                "bg": "#859900"
            }
        }
    },
    "pasink": {
        "states": {
            "muted": {
                "prefix": "  "
            },
            "unmuted": {
                "prefix": "  "
            }
        }
    },
    "pasource": {
        "states": {
            "muted": {
