            {
                "fg": "#ebdbb2",
                "bg": "#1d2021"
            },
            {
                "fg": "#fbf1c7",
                "bg": "#282828"
            }
        ],
        "fg-critical": "#fbf1c7",
        "bg-critical": "#cc241d",
        "fg-warning": "#1d2021",
