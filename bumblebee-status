#!/usr/bin/env python

import bumblebee.engine

def main():
    args = bumblebee.engine.Arguments()

    engine = bumblebee.engine.Engine(args.args())
    engine.load_modules()
    engine.register_events()

    engine.run()

if __name__ == "__main__":
    main()

# vim: tabstop=8 expandtab shiftwidth=4 softtabstop=4
