#!/usr/bin/env python

import sys
import bumblebee.engine
import bumblebee.config

def main():
    config = bumblebee.config.Config(sys.argv[1:])
    engine = bumblebee.engine.Engine(
        config=config
    )

    engine.run()

if __name__ == "__main__":
    main()

